{
  "manifest_version": 2,

  "name": "Blue Example",
  "description": "This extension makes all text blue on a page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title":"Click Me!"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}